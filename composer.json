{
    "name": "nepada/meta-control",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Nette control for managing meta data in HTML header.",
    "keywords": ["meta", "header", "component", "nette"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/meta-control/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=8.1.0 <8.5",
        "nette/utils": "^3.2@dev || ^4.0@dev",
        "nette/application": "^3.1.4@dev"
    },
    "require-dev": {
        "nette/tester": "2.5.4",
        "phpstan/phpstan": "2.1.1",
        "phpstan/phpstan-strict-rules": "2.0.1",
        "phpstan/phpstan-nette": "2.0.2",
        "nepada/phpstan-nette-tester": "2.0.0",
        "spaze/phpstan-disallowed-calls": "4.1.1",
        "shipmonk/phpstan-rules": "4.1.0",
        "php-parallel-lint/php-parallel-lint": "1.4.0",
        "nepada/coding-standard": "7.14.1",
        "composer-runtime-api": "^2.0",
        "composer/semver": "3.4.3",
        "nette/http": "*@dev"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\MetaControl\\": "src/MetaControl/"
        },
        "files": ["src/compatibility.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    }
}
